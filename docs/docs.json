{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "BeeAI",
  "colors": {
    "primary": "#000000",
    "light": "#c5c5c5",
    "dark": "#000000"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "groups": [
      {
        "group": "Introduction",
        "pages": [
          "/introduction/welcome",
          "/introduction/quickstart",
          "/introduction/installation"
        ]
      },
      {
        "group": "How To",
        "pages": [
          "/how-to/run-agents",
          "/how-to/compose-agents",
          "/how-to/build-agents",
          "/how-to/import-agents",
          "/how-to/share-agents"
        ]
      },
      {
        "group": "Core Concepts",
        "pages": ["/concepts/architecture", "/concepts/agents"]
      },
      {
        "group": "Agents",
        "pages": ["/agents/official-list", "/agents/community-list"]
      },
      {
        "group": "Agent Communication Protocol",
        "pages": ["/acp-docs"]
      },
      {
        "group": "Observability & Telemetry",
        "pages": [
          "/observability/agents-traceability",
          "/observability/telemetry",
          "/observability/custom-agent-traceability"
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/beeai-lockup.svg",
    "dark": "/logo/beeai-lockup-white.svg"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "GitHub",
      "href": "https://github.com/i-am-bee/beeai"
    },
    "links": [
      {
        "label": "Agent Catalog",
        "href": "https://beeai.dev/agents"
      },
      { "label": "Discord", "href": "https://discord.gg/AZFrp3UF5k" }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/i-am-bee/beeai",
      "discord": "https://discord.gg/AZFrp3UF5k",
      "bluesky": "https://bsky.app/profile/beeaiagents.bsky.social",
      "youtube": "https://www.youtube.com/@BeeAIAgents"
    }
  },
  "redirects": [
    {
      "source": "/acp/spec/openapi.yaml",
      "destination": "https://github.com/i-am-bee/acp/blob/main/docs/spec/openapi.yaml"
    },
    {
      "source": "/acp/:slug*",
      "destination": "https://agentcommunicationprotocol.dev"
    }
  ]
}
